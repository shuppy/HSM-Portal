    <script src="~/Scripts/jquery.min.js"></script>
    <!-- Google Analytics -->
    <script type="text/javascript">

        var _gaq = _gaq || [];
        _gaq.push(['_setAccount', 'UA-36057737-1']);
        _gaq.push(['_trackPageview']);

        (function () {
            var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
            ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
            var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
        })();

    </script>

    <script src="~/Scripts/less.min.js"></script>
</head>
<body class="">
    
    <!-- Main Container Fluid -->
    <div class="container-fluid fluid menu-left" style="visibility:visible">

        <!-- Top navbar -->
        <div class="navbar main hidden-print">

            <!-- Brand -->
            
            <a href="../" class="appbrand pull-left"><span>HSoG&reg; Portal - &copy; @DateTime.Now.Year  <span>v1</span></span></a>

            <!-- Menu Toggle Button -->
            <button type="button" class="btn btn-navbar">
                <span class="icon-bar"></span> <span class="icon-bar"></span> <span class="icon-bar"></span>
            </button>
            <!-- // Menu Toggle Button END -->
            <!-- Top Menu -->
            <ul class="topnav pull-left tn1">

                <li class="hidden-phone">

                    <a class="glyphicons brightness_increase" href="../"><i></i>The Healing Streams of God.</a>
                </li>

            </ul>
            <!-- // Top Menu END -->
            <!-- Top Menu Right -->
            <ul class="topnav pull-right">

                <!-- Language menu -->
                
                <!-- // Dropdown END -->
                <!-- Profile / Logout menu -->
                <li class="account">
                    <a data-toggle="dropdown" href="#" class="glyphicons logout lock"><span class="hidden-phone text">User Account</span><i></i></a>
                    <ul class="dropdown-menu pull-right">
                        <li><a href="#" class="glyphicons cogwheel">Settings<i></i></a></li>
                        <li class="highlight profile">
                            <span>
                                <span class="heading">Profile <a href="#" class="pull-right">edit</a></span>
                                <span class="img"></span>
                                <span class="details">
                                    <a href="#">@Html.Partial("_LoginPartial")</a>
                                    contact@healingstreams.com
                                </span>
                                <span class="clearfix"></span>
                            </span>
                        </li>
                        <li>
                            <span>
                                <a class="btn btn-default btn-mini pull-right" href="#">
                                    @Html.Partial("_LoginPartial")
                                </a>
                            </span>
                        </li>
                    </ul>
                </li>
                <!-- // Profile / Logout menu END -->

            </ul>
            <!-- // Top Menu Right END -->


        </div>
        <!-- Top navbar END -->
        <!-- Sidebar menu & content wrapper -->
        <div id="wrapper">

            <!-- Sidebar Menu -->
            <div id="menu" class="hidden-phone hidden-print">

                <!-- Scrollable menu wrapper with Maximum height -->
                <div class="slim-scroll" data-scroll-height="800px">

                    <!-- Sidebar Profile -->
                    <span class="profile">
                        <a class="img" href="#">
                           
                        </a>
                        <span>
                            <strong>Welcome</strong>
                            <a href="#" class="glyphicons right_arrow">
                                <i>
                                    @Html.Partial("_LoginPartial")
                                </i>
                            </a>
                        </span>
                    </span>
                    <!-- // Sidebar Profile END -->
                    <!-- Sidebar Mini Stats -->
                    <!-- // Sidebar Mini Stats END -->
                    <!-- Regular Size Menu -->
                    <ul>
                        <!-- Menu Regular Item -->
                        
                        <!-- Components Submenu Level 1 -->
                        <li class="hasSubmenu">
                            <a data-toggle="collapse" href="#members" class="glyphicons user"><i></i><span>Membership</span></a>
                           <ul class="collapse" id="members">
                               <li class="">@Html.ActionLink("Members List","Index","Members") </li>
                               <li class="">@Html.ActionLink("Split Management","Reshuffle","ChoirSplits")</li>
                               <li class="">@Html.ActionLink("Friendship Mgt.","Reshuffle","FriendshipGroups")</li>
                           </ul>
                            <span class="count">3</span>
                        </li>
                        <!-- Components Submenu Level 1 END -->
                        <!-- Landing Submenu Level 1 -->
                        <li class="hasSubmenu glyphicons fabric" >
                            <a data-toggle="collapse" href="#wardrobe"><i></i><span>Wardrobe</span></a>
                        </li>
                        <!-- // Gallery Submenu Level 1 END -->
                        <!-- Extra Submenu Level 1 -->
                        <li class="hasSubmenu glyphicons coins"  >
                            <a data-toggle="collapse" href="#finance"><i></i><span>Finance</span></a>
                        </li>
                        <!-- // Extra Submenu Level 1 END -->
                        <li class="hasSubmenu" >
                            <a class="glyphicons stats" data-toggle="collapse" href="#reports"><i></i><span>Reports</span></a>
                            <ul class="collapse" id="reports">
                                <li class="">@Html.ActionLink("Members Report","Index","Reports")</li>
                                @*<li class="">Financial Reports</li>
                                <li class="">Wardrobe Reports</li>*@
                                
                            </ul>
                            <span class="count">1</span>
                        </li>
                        <!-- Gallery Submenu Level 1 -->
                        <li class="hasSubmenu" >
                            <a data-toggle="collapse" href="#settings" class="glyphicons settings"><i></i><span>Settings</span></a>
                            <ul class="collapse" id="settings">
                                <li class="">@Html.ActionLink("Academy","Index","Academy")</li>
                                <li class="">@Html.ActionLink("Choir Parts","Index","ChoirParts")</li>
                                <li class="">@Html.ActionLink("Availability","Index","Availability")</li>
                                <li class="">@Html.ActionLink("Choir Splits","Index","ChoirSplits")</li>
                                <li class="">@Html.ActionLink("Friendship","Index","FriendshipGroups")</li>
                                <li class="">@Html.ActionLink("Occupations", "Index", "Occupations")</li>
                                <li class="">@Html.ActionLink("Positions", "Index", "Posts")</li>
                            </ul>
                            <span class="count">7</span>
                        </li>
                    </ul>

                    <div class="clearfix"></div>


                </div>
                <!-- // Scrollable Menu wrapper with Maximum Height END -->

            </div>
            <!-- // Sidebar Menu END -->
            <!-- Content -->
            <div id="content">
                <!-- Filters -->
                @RenderBody()

                <!-- // Footer END -->
                <footer>
                    <p>&copy; @DateTime.Now.Year - HSoG Portal</p>
                </footer>
            </div>
            

        </div>
        @*<script type="text/javascript" src="../Scripts/jsapi"></script>*@
